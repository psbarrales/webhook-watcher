{
  "name": "boilerplate-app",
  "description": "BoilerPlate App",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "url": "https://github.com/psbarrales/boilerplate-react-app.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "test": "vitest run",
    "test:e2e": "VITE_ENVIRONMENT=test start-server-and-test dev http://localhost:5173 cypress:run",
    "test:e2e:open": "VITE_ENVIRONMENT=test start-server-and-test dev http://localhost:5173 cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress": "cypress open",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint --fix ./src --ext .ts,.tsx",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint"
  },
  "dependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-query": "^5.69.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^11.0.2",
    "framer-motion": "^11.11.17",
    "inherits": "^2.0.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.482.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "styled-components": "^6.1.13",
    "swiper": "^11.1.15",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.14",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.25.9",
    "@badeball/cypress-cucumber-preprocessor": "^21.0.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3",
    "@chromatic-com/storybook": "^3.2.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@tanstack/react-query-devtools": "^5.69.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.13.10",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-cucumber": "^2.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.11.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "start-server-and-test": "^2.0.8",
    "storybook": "^8.4.5",
    "terser": "^5.4.0",
    "tsify": "^5.0.4",
    "typescript": "^5.6.3",
    "vite": "^6.3.6",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.9"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/Tests"
  }
}
